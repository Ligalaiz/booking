{
  "name": "@ligalaiz/booking",
  "version": "0.0.1",
  "description": "Booking",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --profile --json=compilation-stats.json",
    "start:client": "webpack serve --config config/webpack.dev.js",
    "lint": "eslint -c .eslintrc.js ./src --ext .js, --cache",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist",
    "test": "jest --forceExit",
    "test:smoke": "jest --forceExit --t=#smoke",
    "test:test": "jest --forceExit --t=#test",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ligalaiz/booking.git"
  },
  "keywords": [
    "booking",
    "ticket"
  ],
  "author": "Ligalaiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ligalaiz/booking/issues"
  },
  "homepage": "https://github.com/Ligalaiz/booking#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.8",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.31",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "*",
    "compile-ejs-loader": "^0.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.0.7",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-localstorage-mock": "^2.4.17",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.4.3",
    "npm": "^8.3.2",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.1",
    "sugarss": "^4.0.1",
    "svgo": "^2.3.1",
    "webpack": "^5.47.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm test:smoke"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "npm run test:smoke"
    ]
  }
}
